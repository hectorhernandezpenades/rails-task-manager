<h1>My Tasks</h1>
<ul>
  <% @tasks.each do |task| %>
    <% if task.completed == true %>
      <li><i class="far fa-check-square"></i><%= link_to task.title, task_path(task) %></li>
    <% else %>
      <li><i class="far fa-square"></i><%= link_to task.title, task_path(task) %><br></li>
    <% end %>
  <% end %>
</ul>
